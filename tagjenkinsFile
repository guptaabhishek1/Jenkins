import hudson.model.User
import groovy.json.JsonSlurper
import groovy.json.JsonOutput
pipeline 
{
  agent any
  stages { 
    stage('TagEC2Resources') {
      steps{
        script{
          echo "${params.InstanceId}"
          if (params.InstanceId == ""){
            echo "Inside if block"
            params.InstanceId = "None"
            echo "${params.InstanceId}"
          }
          if (params.AutoScalingGroupId != null){
            echo "Inside ASG If block"
            params.AutoScalingGroupId = "None"
            echo "${params.AutoScalingGroupId}"
          }
          //calling python script
          sh "python DeathTimeTag.py ${params.InstanceId} ${params.AutoScalingGroupId}"
        }
      }
    }
  }
}
