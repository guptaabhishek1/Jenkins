import hudson.model.User
import groovy.json.JsonSlurper
import groovy.json.JsonOutput
pipeline 
{
  agent any
  stages { 
    stage('TagEC2Resources') {
      steps{
        script{
          echo "${params.InstanceId}"
          for (instance in params.InstanceId) {
            echo "${instance}"
          }
        }
      }
    }
  }
}
